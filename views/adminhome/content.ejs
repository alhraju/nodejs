            </ul> 
			
            <ul>
                <li><a href="/logout">Logout</a></li>
            </ul>
        </td>
        <td valign="top">


		
	<table border="1" cellpadding="5" cellspacing="0">
		<tr><td colspan="4" align="CENTER">Content List</td></tr>
		<tr>
			<td>ID</td><td>NAME</td><td>File</td><td>Type</td><td>Action</td>
		</tr>
		<% for(var i=0; i < uList.length; i++){ %>
		<tr>
			<td><%= uList[i].id %></td>
			<td><%= uList[i].name %></td>
            <td><%= uList[i].file %></td>
			<td><%= uList[i].type %></td>
			<td>
				<a href="/adminhome/contentdelete/<%=uList[i].id%>"> DELETE</a>
			</td>
		</tr>
		<% } %>
	</table>
	
	</td>
    </tr>
    <tr>
        <td colspan="2" align="center">
            Copyright &copy; 2019 (AIUB-ATP3)
        </td>
    </tr>
</table>
</body>
</html>